<% provide(:title, '推客群列表') %>
<h1>推客群列表</h1>

<%= will_paginate previous_label: '上一页', next_label: '下一页' %>
<table class="table table-striped">
  <tr>
    <th></th>
    <th>名称</th>
    <th>合同号</th>
    <th>注册码</th>
    <th>管理员</th>
    <th>会员总数</th>
    <th>商品总数</th>
    <th>URL</th>
    <th>Token</th>
    <th>创建时间</th>
    <th><%= link_to '新增', new_organization_path %></th>
  </tr>
  <tbody>
	  <ul class="organizations">
	    <%= render @organizations %>
	  </ul>
  </tbody>
</table>
<%= will_paginate previous_label: '上一页', next_label: '下一页' %>
<p><%= link_to '新增', new_organization_path %></p>


